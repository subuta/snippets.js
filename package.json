{
  "name": "@subuta/snippets",
  "version": "0.3.0",
  "description": "my javascript re-usable snippets used along with https://github.com/subuta/matryoshka.js :)",
  "scripts": {},
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "source": [
      "lib/**/*.js"
    ],
    "failFast": true,
    "failWithoutAssertions": false,
    "babel": {
      "extends": ".babelrc",
      "plugins": [
        [
          "module-resolver",
          {
            "root": [
              "./"
            ],
            "alias": {
              "test": "./test"
            }
          }
        ]
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subuta/snippets.js.git"
  },
  "author": "subuta <shunta.3110@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/subuta/snippets.js/issues"
  },
  "homepage": "https://github.com/subuta/snippets.js#readme",
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "common-tags": "^1.7.2"
  },
  "dependencies": {
    "api-check": "^7.5.5",
    "bld.js": "^0.2.1",
    "lodash": "^4.17.4",
    "path-to-regexp": "^2.1.0",
    "pluralize": "^7.0.0"
  }
}
