// Jest Snapshot v1, https://goo.gl/fbAQLP

exports[`koa/api-test/actions/Show should Show koa Show action test 1`] = `
"test('show should return user', async (t) => {
  const {request} = t.context

  // mock jwks
  const token = createToken(privateKey, '123', currentUser)
  jwksEndpoint('http://localhost', [{pub: publicKey, kid: '123'}])

  const response = await request
    .get('/api/users/2054')
    .set('Authorization', \`Bearer \${token}\`)

  t.is(response.status, 200)

  t.deepEqual(response.body.id, 2054)
  t.deepEqual(response.body.auth0Id, 47736)
  t.deepEqual(response.body.locale, 'en')
  t.deepEqual(response.body.nickname, 'Avon Buckinghamshire')
  t.deepEqual(response.body.status, 'Practical')
  t.deepEqual(
    response.body.avatar,
    'https://s3.amazonaws.com/uifaces/faces/twitter/petrangr/128.jpg'
  )
  t.deepEqual(response.body.created_at, '2000-03-15T05:45:28.720Z')
  t.deepEqual(response.body.updated_at, '2000-09-21T19:03:58.079Z')
})
"
`;
